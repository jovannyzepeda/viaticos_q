<div class="col-md-6 col-md-offset-3">
  <h1 class="center">Generacion de archivo XLS</h1>
  <form method="get" action="/zones/1/proyects/30/spends/">
    <div class="form-group date">
      <label for = 'inicio'>Fecha Inicio:</label>
      <%= date_field(:xls,:inicio) %>
    </div>
    <div class="form-group date">
      <label for= "fin">Fecha Fin:</label>
      <%= date_field(:xls,:fin)%>
    </div>
    <div class="actions">
      <input class="btn btn-primary btn-sm guardar" type="submit"></input>
    </div>
  </form>

  <%= link_to ' Regresar', [@zone,@proyect], class: "btn btn-info float_right " %>
</div>
